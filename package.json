{
	"name": "funkytime-stream",
	"version": "1.0.0",
	"description": "Stream data updates from funkytime.tv.",
	"repository": "https://github.com/Richienb/funkytime-stream.git",
	"author": "Richie Bendall <richiebendall@gmail.com>",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"lint": "xo",
		"test": "yarn lint && ava"
