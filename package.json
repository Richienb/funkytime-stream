		"api",
		"stream",
		"events",
		"emitter",
		"check"
	],
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"got": "^10.3.0",
		"ow": "^0.15.1"
	},
	"devDependencies": {
		"ava": "^3.0.0",
		"eslint-config-richienb": "^0.3.0",
		"xo": "^0.25.3"
	},
	"resolutions": {
		"eslint": "^6.8.0"
	},
	"xo": {
		"extends": "richienb/node",
		"rules": {
			"promise/prefer-await-to-then": 0,
			"promise/always-return": 0
		}
	}
